  "license": "MIT",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "stats": "npm run build -- --profile --json > stats.json",
    "dev": "node server.js",
    "lint": "eslint app/ --ext .js --ext .jsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha",
    "test:watch": "npm run test -- --watch --reporter min",
    "test:ci": "npm run test -- --reporter mocha-circleci-reporter",
    "postinstall": "npm run build",
    "start": "echo 'Not Implemented!' && exit 1",
    "cov": "nyc --extension .jsx npm run test"
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-airbnb": "^2.0.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "html-webpack-plugin": "^2.16.2",
    "immutable": "^3.7.6",
    "node-sass": "^3.4.2",
    "nyc": "^6.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "webpack-merge": "^0.12.0"
  },
  "dependencies": {},
  "engines": {
    "node": "^5.7"
  },
  "nyc": {
    "exclude": [
      "app/**/*-test.js",
      "lib"
    ]
  }
